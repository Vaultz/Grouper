<p id="notice"><%= notice %></p>
<%= form_for @workshop, url: wizard_path, method: :put do |f| %>
<ul>
<%= f.fields_for :projects do |projects_form| %>
      <li>
        <%= projects_form.label :name %>
        <%= projects_form.text_field :name %>

        <%= projects_form.label :description %>
        <%= projects_form.text_field :description %>
      </li>
    <% end %>
</ul>
<div class="actions">
  <%= submit_tag('Valider') %>
</div>
<% end %>
